# Change default port to not clash with the orchestration cluster
SERVER_PORT=9090
LOGGING_LEVEL_IO_CAMUNDA_CONNECTOR=ERROR

# Configure connection to orchestration cluster
CAMUNDA_CLIENT_RESTADDRESS=http://127.0.0.1:8080
CAMUNDA_CLIENT_GRPCADDRESS=http://127.0.0.1:26500
# Will be replaced with identity when ready
# https://github.com/camunda/connectors/tree/main/connector-runtime#configuration-options
CAMUNDA_CLIENT_MODE=selfManaged
CAMUNDA_CLIENT_AUTH_METHOD=basic
CAMUNDA_CLIENT_AUTH_USERNAME=demo
CAMUNDA_CLIENT_AUTH_PASSWORD=demo
