# Example backend configuration for Cognito test module
# Copy this file to backend.tf and customize for your environment

terraform {
  backend "s3" {
    bucket  = "your-terraform-state-bucket"
    key     = "cognito-test/your-cluster-name/terraform.tfstate"
    region  = "eu-central-1"
    profile = "your-aws-profile"
  }
}
